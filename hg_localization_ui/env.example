# HG-Localization UI Configuration
# Copy this file to .env and fill in your values

# S3 Configuration (Required)
HGLOC_S3_BUCKET_NAME=your-s3-bucket-name

# S3 Endpoint (Optional - leave empty for AWS S3)
HGLOC_S3_ENDPOINT_URL=

# AWS Credentials (Optional for public access, required for private access)
HGLOC_AWS_ACCESS_KEY_ID=
HGLOC_AWS_SECRET_ACCESS_KEY=

# AWS Region (Optional)
HGLOC_AWS_DEFAULT_REGION=us-east-1

# S3 Data Prefix (Optional - namespace within bucket)
HGLOC_S3_DATA_PREFIX=

# Local Dataset Storage Path (Optional)
HGLOC_DATASETS_STORE_PATH=

# Development Settings
REACT_APP_API_URL=http://localhost:8000/api 